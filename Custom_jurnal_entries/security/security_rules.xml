<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Definisi grup -->
        <record id="module_account_move_debit_credit_category" model="ir.module.category">
            <field name="name">Account Move Debit Credit</field>
            <field name="description">User access level for Account Move Debit Credit</field>
            <field name="sequence">20</field>
        </record>

        <!-- Multi-company rule -->
        <record id="account_move_debit_credit_company_rule" model="ir.rule">
            <field name="name">Account Move Debit Credit Company Rule</field>
            <field name="model_id" search="[('model', '=', 'account.move.debit.credit')]"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
            <field name="global" eval="True"/>
        </record>
    </data>
</odoo> 